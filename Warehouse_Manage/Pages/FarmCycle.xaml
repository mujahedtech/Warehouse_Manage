                            <DatePicker Width="120" Foreground="Black" Name="txtCarpentryDate" SelectedDate="{x:Static sys:DateTime.Now}" Grid.Row="3" Grid.Column="3" />
                            <Button Click="btnCarpentryAdd_Click" Name="btnCarpentryAdd" Content="اضافة"       Grid.Row="3" Grid.Column="9" Style="{StaticResource StandardButtonSound}" Height="30" VerticalAlignment="Bottom" />


                            <Grid  Visibility="{Binding ElementName=GridHideControll,Path=Visibility}" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="11" Grid.RowSpan="5" Background="White"/>




                            <DataGrid  Name="ListCarpentrys" Style="{StaticResource DataGridStyle }"   Grid.Row="5" Grid.Column="1" Grid.ColumnSpan="9"  >

                                <DataGrid.Resources>



                                    <Style BasedOn="{StaticResource {x:Type DataGridColumnHeader}}" TargetType="{x:Type DataGridColumnHeader}">

                                        <Setter Property="HorizontalContentAlignment" Value="Center" />
                                        <Setter Property="Height" Value="50"/>


                                        <Setter Property="Template">
                                            <Setter.Value>
                                                <ControlTemplate >
                                                    <Border CornerRadius="5" Background="#ECF2FF">
                                                        <TextBlock Margin="5,0" Text="{Binding }"
                                               FontSize="14"
                                               FontFamily="Nunito Sans"
                                               FontWeight="SemiBold"
                                               HorizontalAlignment="Center"
                                               VerticalAlignment="Center"
                                                         
                                               />

                                                    </Border>

                                                </ControlTemplate>
                                            </Setter.Value>
                                        </Setter>




                                    </Style>

                                </DataGrid.Resources>

                                <DataGrid.Columns>


                                    <DataGridTemplateColumn Header="رقم تسلسل" >
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>

                                                <TextBlock HorizontalAlignment="Center" Text="{Binding CarpentryID}" Style="{StaticResource textblockEdit}"/>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>


                                    <DataGridTemplateColumn Header="عدد النجارة" >
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>

                                                <TextBlock HorizontalAlignment="Center" Text="{Binding CarpentryCount}" Style="{StaticResource textblockEdit}"/>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>
                                    <DataGridTemplateColumn Header="تاريخ الادخال" >
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>

                                                <TextBlock HorizontalAlignment="Center" Text="{Binding DateAdd}" Style="{StaticResource textblockEdit}"/>
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>

                                    <DataGridTemplateColumn  Width="50">
                                        <DataGridTemplateColumn.CellTemplate>
                                            <DataTemplate>

                                                <Button Click="Delete_Click"  CommandParameter="{Binding .}" FontFamily="Carpentrys" Background="Red" HorizontalAlignment="Center" />
                                            </DataTemplate>
                                        </DataGridTemplateColumn.CellTemplate>
                                    </DataGridTemplateColumn>



