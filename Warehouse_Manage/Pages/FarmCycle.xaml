                                <ColumnDefinition/>
                                <ColumnDefinition Width="{StaticResource ColumnsSpaceMargin}"/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="{StaticResource ColumnsFirstWidth}"/>
                            </Grid.ColumnDefinitions>


