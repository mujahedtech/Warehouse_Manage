                                        <TextBlock HorizontalAlignment="Center" Text="{Binding FarmStrNumber}" Style="{StaticResource textblockEdit}"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>

                            <DataGridTemplateColumn Header="رقم المزرعة" >
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>

                                        <TextBlock HorizontalAlignment="Center" Text="{Binding FarmID}" Style="{StaticResource textblockEdit}"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>

                            <DataGridTemplateColumn Header="اسم المزرعة" >
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>

                                        <TextBlock HorizontalAlignment="Center" Text="{Binding FarmName}" Style="{StaticResource textblockEdit}"/>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            
                            
                            <DataGridTemplateColumn Header="تاريخ بداية الدورة" >
                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>

                                        <TextBlock HorizontalAlignment="Center" Text="{Binding DateStart}" Style="{StaticResource textblockEdit}"/>
